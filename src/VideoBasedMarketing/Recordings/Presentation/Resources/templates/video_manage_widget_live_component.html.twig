                                {# Heroicon: share solid #}
                                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor" class="w-5 h-5">
                                    <path fill-rule="evenodd" d="M15.75 4.5a3 3 0 11.825 2.066l-8.421 4.679a3.002 3.002 0 010 1.51l8.421 4.679a3 3 0 11-.729 1.31l-8.421-4.678a3 3 0 110-4.132l8.421-4.679a3 3 0 01-.096-.755z" clip-rule="evenodd" />
                                </svg>
                                {{ 'video_manage_widget.edit_modal.cta_share'|trans }}
                            </button>
                        {% endif %}

                        <a
                                {% if TwigHelperService.capabilitiesService.mustBeForcedToClaimUnregisteredUser(app.user) %}
                                    href="{{ claimUnregisteredUserLandingpagePath }}"
                                {% else %}
                                    href="{{ TwigHelperService.recordingsPresentationService.videoShareLinkUrl(this.video) }}"
                                {% endif %}

                                target="_blank"
                                class="cta-with-icon-primary"
                        >
                            {# Heroicon: eye solid #}
                            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor" class="w-5 h-5">
                                <path d="M12 15a3 3 0 100-6 3 3 0 000 6z" />
                                <path fill-rule="evenodd" d="M1.323 11.447C2.811 6.976 7.028 3.75 12.001 3.75c4.97 0 9.185 3.223 10.675 7.69.12.362.12.752 0 1.113-1.487 4.471-5.705 7.697-10.677 7.697-4.97 0-9.186-3.223-10.675-7.69a1.762 1.762 0 010-1.113zM17.25 12a5.25 5.25 0 11-10.5 0 5.25 5.25 0 0110.5 0z" clip-rule="evenodd" />
                            </svg>
                            {{ 'video_manage_widget.edit_modal.cta_preview'|trans }}
                        </a>

                        {% if TwigHelperService.capabilitiesService.mustBeForcedToClaimUnregisteredUser(app.user) %}
                            <a
                                    class="cta-with-icon-success"
                                    href="{{ path('videobasedmarketing.account.presentation.claim_unregistered_user.landingpage') }}"
                            >
                                {# Heroicon: check-circle solid #}
                                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor" class="w-5 h-5">
                                    <path fill-rule="evenodd" d="M2.25 12c0-5.385 4.365-9.75 9.75-9.75s9.75 4.365 9.75 9.75-4.365 9.75-9.75 9.75S2.25 17.385 2.25 12zm13.36-1.814a.75.75 0 10-1.22-.872l-3.236 4.53L9.53 12.22a.75.75 0 00-1.06 1.06l2.25 2.25a.75.75 0 001.14-.094l3.75-5.25z" clip-rule="evenodd" />
                                </svg>
                                {{ 'video_manage_widget.edit_modal.cta_done'|trans }}
                            </a>

                        {% else %}
                            {% if this.doneCtaMustRedirectToOverview %}
                                <a
                                        class="cta-with-icon-success"
                                        href="{{ path('videobasedmarketing.recordings.presentation.videos.overview') }}"
                                >
                                    {# Heroicon: check-circle solid #}
                                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor" class="w-5 h-5">
                                        <path fill-rule="evenodd" d="M2.25 12c0-5.385 4.365-9.75 9.75-9.75s9.75 4.365 9.75 9.75-4.365 9.75-9.75 9.75S2.25 17.385 2.25 12zm13.36-1.814a.75.75 0 10-1.22-.872l-3.236 4.53L9.53 12.22a.75.75 0 00-1.06 1.06l2.25 2.25a.75.75 0 001.14-.094l3.75-5.25z" clip-rule="evenodd" />
                                    </svg>
                                    {{ 'video_manage_widget.edit_modal.cta_done'|trans }}
                                </a>
                            {% else %}
                                <button
                                        class="cta-with-icon-success"
                                        data-action="live#action"
                                        data-action-name="hideEditModal"
                                >
                                    {# Heroicon: check-circle solid #}
                                    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="currentColor" class="w-5 h-5">
                                        <path fill-rule="evenodd" d="M2.25 12c0-5.385 4.365-9.75 9.75-9.75s9.75 4.365 9.75 9.75-4.365 9.75-9.75 9.75S2.25 17.385 2.25 12zm13.36-1.814a.75.75 0 10-1.22-.872l-3.236 4.53L9.53 12.22a.75.75 0 00-1.06 1.06l2.25 2.25a.75.75 0 001.14-.094l3.75-5.25z" clip-rule="evenodd" />
                                    </svg>
                                    {{ 'video_manage_widget.edit_modal.cta_done'|trans }}
                                </button>
                            {% endif %}

                        {% endif %}

                    {% endblock %}

                {% endembed %}
            </div>
        </div>
    {% endif %}

</div>
